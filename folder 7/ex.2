#include <stdio.h>
 
int main()
{
    int arr [] = {1, 2, 3, 4};
    
    arr [0] = arr [0] * 2; //увеличиваем 1 и 4 элементы массива в 2 раза
    arr [3] = arr [3] * 2;
    
    for (int i = 0; i < 4; i++) //для вывода всего массива необходимо использовать цикл(((((
    {
        printf("%d\n", arr [i]);
    }
    
    return 0;
}
