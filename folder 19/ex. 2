#include <stdio.h>

typedef struct 
{
    char * name;
    int age;
    double salary;
} employee;
  
int main(void)
{
    employee sam = {"Sam", 23, 4500};
    employee bob = {"Bob", 50, 5500};
    
    printf("Name:%s \t Age: %d\n Salary: %.2lf\n", sam.name, sam.age, sam.salary);
    printf("Name:%s \t Age: %d\n Salary: %.2lf\n", bob.name, bob.age, bob.salary);
    return 0;
}
