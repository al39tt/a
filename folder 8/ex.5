#include <stdio.h>

int main() {
    float lengthfeet, lengthmeters;

    printf("length in feet: "); //ввод в футах
    scanf("%f", &lengthfeet);

    lengthmeters = lengthfeet * 3.281; // перевод в метры

    printf(" length in meters = %.3f * 3.281 = %.3f m^2\n", lengthfeet, lengthmeters);

    return 0;
}
